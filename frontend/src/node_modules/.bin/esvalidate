#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/gin-coraza-waf/frontend/src/node_modules/esprima/bin/node_modules:/root/gin-coraza-waf/frontend/src/node_modules/esprima/node_modules:/root/gin-coraza-waf/frontend/src/node_modules:/root/gin-coraza-waf/frontend/node_modules:/root/gin-coraza-waf/node_modules:/root/node_modules:/node_modules:/root/gin-coraza-waf/frontend/src/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/root/gin-coraza-waf/frontend/src/node_modules/esprima/bin/node_modules:/root/gin-coraza-waf/frontend/src/node_modules/esprima/node_modules:/root/gin-coraza-waf/frontend/src/node_modules:/root/gin-coraza-waf/frontend/node_modules:/root/gin-coraza-waf/node_modules:/root/node_modules:/node_modules:/root/gin-coraza-waf/frontend/src/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esprima/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../esprima/bin/esvalidate.js" "$@"
fi
